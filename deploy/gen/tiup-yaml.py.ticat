help = generate tiup yaml config from env values written by deploy.set.* or deploy.auto.allocator.*
abbr = tiup|yaml

[arg]
depose-env-kvs-after-deployed|depose-env = false

[arg2env]
deploy.env.kvs.depose-after-deployed = depose-env-kvs-after-deployed

[env]
tidb.tiup.yaml = write
deploy.env.kvs.depose-after-deployed = read
