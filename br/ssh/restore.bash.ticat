help = restore instances' data dir from <dir>.<tag>
abbrs = rs

[arg]
tag|t = ''
cluster-name|cluster|name|n = ''
deploy-to-user|user|u = tidb
deploy-user = root

[arg2env]
tidb.data.tag = tag
tidb.cluster = cluster-name
deploy.to-user = deploy-to-user
deploy.user = deploy-user

[val2env]
tidb.need-init = false

[env]
tidb.data.tag = read
tidb.cluster = read
deploy.to-user = read
deploy.user = read

[dep]
tiup = to get dir of each instance from cluster
