help = execute sql in mysql
abbrs = exec|exe

[args]
query|sql|q = ''
db-name|database|db = ''
host|h = 127.0.0.1
port|p = 4000
user|u = root
password|pwd = ''
output-format|format|fmt = ''
warnings|warning|warn = true

[env]
mysql.host = read
mysql.port = read
mysql.user = read

[arg2env]
mysql.host = host
mysql.port = port
mysql.user = user
mysql.pwd = password

[dep]
mysql = as client
