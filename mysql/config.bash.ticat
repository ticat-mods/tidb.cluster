help = setup mysql access info
abbr = conf
cmd = config.bash

[args]
host|h = 127.0.0.1
port|p = 4000
user|u = root
password|pwd = ''
verify-access|verify|v = true

[arg2env]
mysql.host = host
mysql.port = port
mysql.user = user
mysql.pwd = password

[env]
mysql.host = write
mysql.port = write
mysql.user = write
mysql.pwd = may-write

[dep]
mysql = as client
